{
  "Vue3 Setup Template": {
    "scope": "vue",
    "prefix": "vue3setup",
    "body": [
      "<template>",
      "  <div>${1:Vue3 Setup}</div>",
      "</template>",
      "",
      "<script lang=\"ts\" setup>",
      "import { ref } from 'vue'",
      "",
      "const a = ref(1);",
      "</script>",
      "",
      "<style lang=\"less\" scoped>",
      "",
      "</style>"
    ],
    "description": "vue3 setup script template for quickly setting up the .vue file structure"
  },
  "Vue3 Normal Template": {
    "scope": "vue",
    "prefix": "vue3tpl",
    "body": [
      "<template>",
      "  <div>Vue3 Template</div>",
      "</template>",
      "",
      "<script lang=\"ts\">",
      "import { defineComponent, ref } from 'vue'",
      "",
      "export default defineComponent({",
      "  name: '${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}}',",
      "  setup() {",
      "    const a = ref(1);",
      "    return {",
      "      a",
      "    }",
      "  }",
      "})",
      "</script>",
      "",
      "<style lang=\"less\" scoped>",
      "",
      "</style>"
    ],
    "description": "vue3 normal template for quickly setting up the .vue file structure"
  }
}